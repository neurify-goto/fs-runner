{
    "environment_variables": {
        "SUPABASE_URL": {
            "required": true,
            "min_length": 20,
            "url_prefix": "https://",
            "error_msg": "SUPABASE_URL must be a valid Supabase project URL"
        },
        "SUPABASE_SERVICE_ROLE_KEY": {
            "required": true,
            "min_length": 100,
            "key_prefix": "eyJ",
            "error_msg": "SUPABASE_SERVICE_ROLE_KEY must be a valid service role key"
        },
        "GITHUB_ACTIONS": {
            "required": false,
            "allowed_values": [
                "true",
                "false"
            ],
            "error_msg": "GITHUB_ACTIONS must be true or false"
        },
        "FORM_SENDER_ENV": {
            "required": false,
    "allowed_values": [
      "cloud_run",
      "github_actions",
      "gcp_batch",
      "local"
    ],
    "error_msg": "FORM_SENDER_ENV must be cloud_run, github_actions, gcp_batch, or local"
        },
        "FORM_SENDER_LOG_SANITIZE": {
            "required": false,
            "allowed_values": [
                "0",
                "1",
                "true",
                "false",
                "yes",
                "no"
            ],
            "error_msg": "FORM_SENDER_LOG_SANITIZE must be a boolean-like value"
        }
    },
    "form_sender": {
        "max_workers": {
            "type": "integer",
            "min_value": 1,
            "max_value": 10,
            "default": 2
        },
        "batch_size": {
            "type": "integer",
            "min_value": 1,
            "max_value": 100,
            "default": 10
        },
        "max_execution_time_hours": {
            "type": "integer",
            "min_value": 1,
            "max_value": 12,
            "default": 5
        }
    },
    "security": {
        "log_sanitization_enabled": {
            "type": "boolean",
            "default": true
        },
        "github_actions_enhanced_masking": {
            "type": "boolean",
            "default": true
        }
    }
}
